body.stretched-background { 
  // background: url(_/images/temp-bg.jpg) no-repeat center center fixed; 
  // -webkit-background-size: cover;
  // -moz-background-size: cover;
  // -o-background-size: cover;
  // background-size: cover;
  max-width: $wrapper-width;
  margin: 0 auto;
  padding: 0 22px;
}

section {
  color: $brand-light-text;
	&.main-container {
    margin-top: $base-line-height / 2;
    padding: $base-line-height - 2;
    height: auto;
		background: $brand-dark-gray;
      .tile{
        position: relative;
        z-index: 1;
        min-height: 200px;
        line-height: 0;
        padding: 0 0 0 0;
        margin: 0 0 0 0;
        //highlight title bg on hover of tile
        &:hover{
          &:first-child .title{
            color: #414042;
            text-shadow: 1px 1px 1px #F8F9F9;
            background: #ccc7c2; /* Old browsers */
            background: -moz-linear-gradient(left,  #ccc7c2 0%, #a7a3a0 23%, #e0e1e2 48%, #a7a3a0 76%, #a7a9ac 100%); /* FF3.6+ */
            background: -webkit-gradient(linear, left top, right top, color-stop(0%,#ccc7c2), color-stop(23%,#a7a3a0), color-stop(48%,#e0e1e2), color-stop(76%,#a7a3a0), color-stop(100%,#a7a9ac)); /* Chrome,Safari4+ */
            background: -webkit-linear-gradient(left,  #ccc7c2 0%,#a7a3a0 23%,#e0e1e2 48%,#a7a3a0 76%,#a7a9ac 100%); /* Chrome10+,Safari5.1+ */
            background: -o-linear-gradient(left,  #ccc7c2 0%,#a7a3a0 23%,#e0e1e2 48%,#a7a3a0 76%,#a7a9ac 100%); /* Opera 11.10+ */
            background: -ms-linear-gradient(left,  #ccc7c2 0%,#a7a3a0 23%,#e0e1e2 48%,#a7a3a0 76%,#a7a9ac 100%); /* IE10+ */
            background: linear-gradient(to right,  #ccc7c2 0%,#a7a3a0 23%,#e0e1e2 48%,#a7a3a0 76%,#a7a9ac 100%); /* W3C */
            filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ccc7c2', endColorstr='#a7a9ac',GradientType=1 ); /* IE6-9 */
          }
        }

        .title{
          z-index: 2;
          position: absolute;
          bottom: 0;
          left: 0;
          height: 45px;
          line-height: 45px;
          padding-left: 12px;
          font-size: 12px;
          color: #a7a9ab;
          border-right: 1px solid $brand-dark-gray;
          border-bottom: 1px solid $brand-dark-gray;
          -webkit-box-shadow:  0px -4px 4px 0px rgba(0, 0, 0, 0.4);
          box-shadow:  0px -4px 4px 0px rgba(0, 0, 0, 0.4);
          text-shadow: 1px 1px 1px $brand-dark-gray;
          background: rgb(131,127,125); /* Old browsers */
          background: -moz-linear-gradient(left,  rgba(131,127,125,1) 0%, rgba(81,77,79,1) 23%, rgba(131,127,125,1) 48%, rgba(81,77,79,1) 76%, rgba(131,127,125,1) 100%); /* FF3.6+ */
          background: -webkit-gradient(linear, left top, right top, color-stop(0%,rgba(131,127,125,1)), color-stop(23%,rgba(81,77,79,1)), color-stop(48%,rgba(131,127,125,1)), color-stop(76%,rgba(81,77,79,1)), color-stop(100%,rgba(131,127,125,1))); /* Chrome,Safari4+ */
          background: -webkit-linear-gradient(left,  rgba(131,127,125,1) 0%,rgba(81,77,79,1) 23%,rgba(131,127,125,1) 48%,rgba(81,77,79,1) 76%,rgba(131,127,125,1) 100%); /* Chrome10+,Safari5.1+ */
          background: -o-linear-gradient(left,  rgba(131,127,125,1) 0%,rgba(81,77,79,1) 23%,rgba(131,127,125,1) 48%,rgba(81,77,79,1) 76%,rgba(131,127,125,1) 100%); /* Opera 11.10+ */
          background: -ms-linear-gradient(left,  rgba(131,127,125,1) 0%,rgba(81,77,79,1) 23%,rgba(131,127,125,1) 48%,rgba(81,77,79,1) 76%,rgba(131,127,125,1) 100%); /* IE10+ */
          background: linear-gradient(to right,  rgba(131,127,125,1) 0%,rgba(81,77,79,1) 23%,rgba(131,127,125,1) 48%,rgba(81,77,79,1) 76%,rgba(131,127,125,1) 100%); /* W3C */
          filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#837f7d', endColorstr='#837f7d',GradientType=1 ); /* IE6-9 */
        }
      }

      .offices{
        position: relative;
        z-index: 1;
        line-height: 0;
        padding: 0 0 0 0;
        margin: 0;
        .map-grid{
          z-index: 0;
          span{
            z-index: 0;
            width: 18px;
            height: 18px;
            background-image: url(./_/images/map-icon-sprite.png);
            background-position: 0 0; 
            i.title{
              z-index: 30;
              font-size: 9px;
              font-style: normal;
              text-transform: uppercase;
              line-height: 27px;
              text-align: center;
              text-shadow: 1px 1px 1px #f8f9f9;
              color: $brand-dark-gray;
              margin-top: -19px;
              margin-left: 9px;
              width: 69px;
              height: 29px;
              display: block;
              background: url(./_/images/map-titles-bg.png);
            }
          }
          span.star{
            background-position: -5px 0;
            position: absolute;
              &:hover{
                cursor: pointer;
                background-position: -43px 0;
              }
            &.denver{
              top: 332px;
              left: 219px;
            }
            &.phoenix{
              top: 346px;
              left: 211px;
            }
          }

          span.dot{
            z-index: 20;
            background-position: -24px 0;
            position: absolute;
              &:hover{
                cursor: pointer;
                background-position: -43px 0;
              }
            &.mexico{   top: 367px; left: 233px;}
            &.sa-1{     top: 464px; left: 342px;}
            &.sa-2{     top: 491px; left: 322px;}
            &.so-af-1{  top: 488px; left: 485px;}
            &.aust{     top: 497px; right: 193px;}
            &.uk{       top: 296px; right: 498px;}
            &.ger{      top: 298px; right: 480px;}
            &.swed{     top: 260px; right: 462px;}
            &.russ{     top: 275px; right: 428px;}
            &.uae{      top: 365px; right: 378px;}
            &.india{    top: 376px; right: 328px;}
            &.sing{     top: 409px; right: 282px;}
            &.china{    top: 347px; right: 245px;}
            &.korea{    top: 338px; right: 228px;}
            &.japan{    top: 336px; right: 207px;}
          }

          //Main post content
          .content{
            position: absolute;
            top: 60px;
            left: 232px;
            z-index: 6;
            p{
              font-size: 18px;
              font-weight: normal;
              font-family: museo_sans100;
              line-height: 25px;
              padding-right: 20px;
            }
            .arrow{
              color: $brand-red;
              font-family: museo_sans300;
              font-size: 22px;
              margin-left: -14px;
              position: relative;
              top: 14px;
              &:before{
                content: "\203A";
              }
            }
            ul{
              margin: $base-line-height 0;
              width: 25%;
              float: left;
              margin-right: 10px;
              &:last-child{
                margin-right: 0;
              }
              li{
                font-size: 12px;
                font-weight: normal;
                font-family: museo_sans100;
                list-style-type: none;
                line-height: $base-line-height + 4;
              }
            }
          .people-title h3{
              font-size: 18px;
              font-weight: normal;
              font-family: museo_sans100;
              line-height: 25px;

            .bio-arrow{
              color: $brand-red;
              font-family: museo_sans300;
              font-size: 22px;
              margin-left: -14px;
              position: relative;
              margin-right: 6px;

              &:before{
                content: "\203A";
              }
            }
              
          }
          .people-bio p{
            padding-right: 20px;
                font-size: 12px;
                font-weight: normal;
                font-family: museo_sans100;
                list-style-type: none;
                line-height: $base-line-height + 4;
              }
          }

        }
      }

    &.min-height{
      min-height: $min-height;
    }

	}
}
/*------------------------------------*\
    media headlines
\*------------------------------------*/
.rh-media{

  .headline{
    height: 40px;
    background: #ccc7c2;
    background: -moz-linear-gradient(left, #ccc7c2 0%, #a7a3a0 14%, #e0e1e2 50%, #a7a3a0 87%, #ccc7c2 100%);
    background: -webkit-gradient(linear, left top, right top, color-stop(0%,#ccc7c2), color-stop(14%,#a7a3a0), color-stop(50%,#e0e1e2), color-stop(87%,#a7a3a0), color-stop(100%,#ccc7c2));
    background: -webkit-linear-gradient(left, #ccc7c2 0%,#a7a3a0 14%,#e0e1e2 50%,#a7a3a0 87%,#ccc7c2 100%);
    background: -o-linear-gradient(left, #ccc7c2 0%,#a7a3a0 14%,#e0e1e2 50%,#a7a3a0 87%,#ccc7c2 100%);
    background: -ms-linear-gradient(left, #ccc7c2 0%,#a7a3a0 14%,#e0e1e2 50%,#a7a3a0 87%,#ccc7c2 100%);
    background: linear-gradient(to right, #ccc7c2 0%,#a7a3a0 14%,#e0e1e2 50%,#a7a3a0 87%,#ccc7c2 100%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ccc7c2', endColorstr='#ccc7c2',GradientType=1 );
    -webkit-box-shadow:  0px -4px 6px 0px rgba(0, 0, 0, 0.1);
    box-shadow:  0px -4px 6px 0px rgba(0, 0, 0, 0.1);

    p{
      font-size: 14px;
      margin-left: 0;
      display: inline-block;
      color: #414042;
      line-height: 40px;
      font-style: italic;
      text-shadow: 1px 1px 1px #F8F9F9;
      &:before{
        font-size: 20px;
        font-style: normal;
        color: $brand-red;
        content: "\203A";
      }
    }
  }
  .website-url{
    line-height: 40px;
    display: inline-block;
    a{
      text-shadow: 1px 1px 1px #888888;
      font-size: 11px;
      font-style: italic;
      text-decoration: none;
      color: #4d4e51;
      display: inline-block;
      &:hover{
        color: $brand-red;
      }
      > span.url-arrow{
          background-image: url(./_/images/url-arrow.png);
          background-position: 0 -2px;
          width: 21px;
          height: 21px;
          margin: 0;
          vertical-align: middle;
          display: inline-block;
      }
    }
  }
}

footer{
  font-size: 1em;
  font-weight: lighter;
  .footer-copyright{
    color: $brand-light-text;
    font-size: .5em;
  }
  .phone-number{
    font-family: 'museo_sans300', sans-serif;
  }
}
//Tile mouseover shadow
#shadow {
  background-image: url(./_/images/shadow.png); 
  opacity: 0.8;
  position: absolute; 
  left: 0; 
  top: 0; 
  width: 100%; 
  z-index: 100;
}

